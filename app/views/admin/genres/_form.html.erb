<%= form_with(model: @genre, url: admin_genres_path) do |form| %>
  <div class = "form-groupe row">
    <label for"name", class="col-sm-2 col-form-label">ジャンル名</label>
    <div class="col-sm-6">
      <%= form.text_field :name,placeholder:"ジャンル名", class:"form-control" %><br>
    </div>
    <div class="col-sm-1">
    <%= form.submit '　新規登録　', class: "btn btn-sm btn-success" %>
    </div>
  </div>
<% end %>